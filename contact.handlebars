 <section id="contact" class="bg-light">
        <div class="contact-form">
          
             
            <h2>Book Your <span class="text-primary">Session</span>Now</h2>
            
            <div class="form-container">
                <div class="mail-icon"><i class="fas fa-envelope"></i></div>
                  <div class="form-box">
                    <h3>Shoot Me a <span class="text-primary">Message</span></h3>
                    <form id="contact-form">
                    <div class="form-group">
                        <label for="first name">First Name</label>
                        <input id="name" type="text" placeholder="Enter Your First Name">
                    </div>
                    <div class="form-group">
                        <label for="last name">Last Name</label>
                        <input id="surname" type="text" placeholder="Enter Your Last Name">
                    </div>
                    <div class="form-group">
                        <label for="email">Email</label>
                        <input id="email" type="email" placeholder="Enter Your Email">
                    </div>
                    <div class="form-group">
                        <label for="message">Message</label>
                        <textarea id="message" rows="7" placeholder="Enter Your First Name"></textarea>
                    </div>

